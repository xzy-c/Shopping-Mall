<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzy.dao.UserLoginMapper">

<select id="userLogin" parameterType="com.xzy.pojo.User" resultType="com.xzy.pojo.User">
    select * from user where user_Name=#{user_Name}
</select>
    <select id="getUser" parameterType="Integer" resultType="com.xzy.vo.Usertext">
        select user_Id,user_Name,user_Password from user where user_Id=#{user_Id}
    </select>
</mapper>